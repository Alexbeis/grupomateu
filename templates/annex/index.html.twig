{% extends 'base.html.twig' %}

{% block body %}
    <section class="content-header">
        <h1>Marcados <small>Listado</small></h1>
        <ol class="breadcrumb">
            <li><a href="{{ path('index_dashboard') }}"><i class="fa fa-dashboard"></i> Dashboard</a></li>
            <li class="active">Marcados</li>
        </ol>
    </section>

    <section class="content">
        {% for code, annex in annexes %}
            {{ include('annex/annex-box.html.twig')}}
        {% else %}
            <div class="row">
                <div class="col-xs-12">
                    <div class="box box-warning">
                        <div class="box-header with-border">
                            <div class="row">
                                <div class="col-xs-12 col-md-8">
                                    <h3 class="box-title">No Hay Animales Marcados</h3>
                                </div>
                                <div class="col-xs-12 col-md-4">

                                </div>
                            </div>
                        </div>
                        <div class="box-body">
                        </div>
                    </div>
                </div>

            </div>
        {% endfor %}

    </section>
{% endblock %}